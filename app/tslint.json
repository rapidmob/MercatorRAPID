{
  "rules": {
    "align": [
      true,
      "parameters",
      "arguments",
      "statements"
    ],
    "ban": false,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "indent": [
      true,
      "tabs"
    ],
    "label-position": true,
    "label-undefined": true,
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-duplicate-key": true,
    "no-shadowed-variable": true,
    "no-duplicate-variable": true, 
    "no-eval": true,
    "no-unreachable": true,
    "one-line": [true,
        "check-open-brace",
        "check-catch",
        "check-else",
        "check-whitespace"
    ],
    "no-trailing-whitespace": true,
    "quotemark": [true, "single"],
    "whitespace": [true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
    ]
  }
}